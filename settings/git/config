# ssh-keygen -t rsa
# chmod 600 id_rsa
# eval `ssh-agent` && ssh-add ~/.ssh/id_rsa
# ssh-add -l
# pbcopy < ~/.ssh/id_rsa.pub
#
# ssh -T git@github.com
